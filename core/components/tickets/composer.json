{
	"name": "modx-pro/tickets",
	"type": "library",
	"require-dev": {
		"modx/revolution": "v3.1.2-pl",
		"sterc/formit": "dev-3.x"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/modxcms/revolution.git"
		},
		{
			"type": "package",
			"package": {
				"name": "sterc/formit",
				"version": "dev-3.x",
				"dist": {
					"url": "https://github.com/Sterc/FormIt/archive/refs/heads/3.x.zip",
					"type": "zip"
				}
			}
		}
	],
	"config": {
		"preferred-install": {
			"*": "dist",
			"modx/revolution": "source"
		}
	},
	"require": {
		"symfony/html-sanitizer": "^7.3"
	}
}
